{
	"compilerOptions": {
		"lib": ["es5", "dom" ],
		"types": ["node", "reflect-metadata"],
		"module": "commonjs",
		"target": "es5",
		"noImplicitAny": false,
		"sourceMap": false,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"outDir": "./dist",
		"baseUrl": "./",
		"paths": {
			/*"@root/*": ["./*"]*/
		}
	},
	"exclude": [
		"test",
		"node_modules",
	]
}
